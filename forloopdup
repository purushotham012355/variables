<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for (let i=2 ;i<=8 ;i=i+1){
            if (i>5){
                continue
                //i=2+1=3; 3+2=5
                //i=3+2=5; 5+2=7
            }
            document.write(i+2)
        }
    </script>
</body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for (let i=1 ;i<=3 ;i++){
            document.write(i,"<br>");

        for (let j=1 ;j<=4 ;j++){
            document.write(j," ");
        }
        document.write("<br>")
        }
    </script>
</body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for(let i=1; i<=5; i++){
            for(let j=1;j<=i;j++){
              document.write("*")
            }
            document.write("<br>")
          }
    </script>
</body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for (let i = 1; i <= 4; i++) {
            for (let j = 1; j <= 4 - i; j++) {
              document.write("&nbsp;");
            }
            for (let k = 1; k <= i; k++) {
              document.write("*"," ");
            }
            document.write("<br>");
          }
         document.write("<br>");
         Pyram
    </script>
</body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for (let i = 1; i <= 6; i++) {
            for (let j = 1; j <= i; j++) {
              document.write("&nbsp;");
            }
            for (let k = 1; k <= 6 - i; k++) {
              document.write("*"," ");
            }
            document.write("<br>");
          }
    </script>
</body>
</html>
